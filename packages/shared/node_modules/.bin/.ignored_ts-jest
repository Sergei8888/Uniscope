#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules/ts-jest/node_modules:/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules:/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules/ts-jest/node_modules:/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules:/Users/sergei888/WebstormProjects/uniscope/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.22.8_jest@29.4.3_typescript@5.1.6/node_modules/ts-jest/cli.js" "$@"
fi
